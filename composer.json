{
  "name": "sereny/nova-searchable-filter",
  "description": "Nova searchable filter for belongsTo relationships.",
  "keywords": [
    "laravel",
    "nova",
    "nova-4",
    "filter",
    "belongs-to"
  ],
  "license": "MIT",
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "laravel/framework": "^9.0",
    "laravel/nova": ">=4.0.0"
  },
  "autoload": {
    "psr-4": {
      "Sereny\\NovaSearchableFilter\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sereny\\NovaSearchableFilter\\FilterServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
